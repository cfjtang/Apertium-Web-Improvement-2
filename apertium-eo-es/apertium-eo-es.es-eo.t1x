    <def-macro n="tipus_verbs" npar="1"> <!-- tots els verbs passen per aqui -->
      <choose>
        <when>
          <test>
          <in caseless="yes">
         <clip pos="1" side="sl" part="lemh"/>
         <list n="agradar"/>
          </in>
          </test>
          <let>
            <var n="tipus_verb"/>
            <lit-tag v="like"/>
          </let>
        </when>
	<when>
    	<test>
     	 <in caseless="yes">
       	  <clip pos="1" side="sl" part="lemh"/>
       	  <list n="moviment"/>
      	</in>
    	</test>
 	<let>
       <var n="tipus_verb"/>
       <lit-tag v="moviment"/>
       </let>
     </when>

    <when>
    <test>
      <in caseless="yes">
         <clip pos="1" side="sl" part="lemh"/>
         <list n="volition"/>
      </in>
    </test>
    <let>
     <var n="tipus_verb"/>
     <lit-tag v="volition"/>
     </let>
    </when>

	<when>
    	<test>
      	<in caseless="yes">
        <clip pos="1" side="sl" part="lemh"/>
        <list n="reportingverb"/>
      	</in>
    	</test>
	<let>
        <var n="tipus_verb"/>
        <lit-tag v="reportingverb"/>
        </let>
	</when>

        <when>
          <test>
            <equal>
              <clip pos="1" side="sl" part="a_verb2"/>
              <lit-tag v="vblex.pron"/>
            </equal>
          </test>
          <let>
            <var n="tipus_verb2"/>
            <lit-tag v="pron"/>
          </let>
        </when>

        <otherwise>
          <let>
            <var n="tipus_verb"/>
            <lit-tag v="res"/>
          </let>
        </otherwise>
      </choose>
    </def-macro>


