<section-def-macros> 


<!--
^dark<adj><sint>$
^dark<adj><sint><comp>$
^dark<adj><sint><sup>$

^malluma<adj><sg><nom>$
^pli<preadv>$ ^malluma<adj><sg><nom>$
^plej<preadv>$ ^malluma<adj><sg><nom>$

-->

<def-macro n="pli_plej_adj" npar="2">
	<choose>
	  <when>
	    <test>
	      <and>
			<equal caseless="yes"><clip pos="1" side="sl" part="lem"/><lit v="pli"/></equal>
      		<equal><clip pos="2" side="tl" part="a_adj"/><lit-tag v="adj.sint"/></equal>
	      </and>
	    </test>
	    <let><var n="adv_adjectiu"/>
			<concat><clip pos="2" side="tl" part="lem"/><clip pos="2" side="tl" part="a_adj"/><lit-tag v="comp"/></concat></let>
	  </when>

	  <when>
	    <test>
	      <and>
          <equal caseless="yes"><clip pos="1" side="sl" part="lem"/><lit v="plej"/></equal>
          <equal><clip pos="2" side="tl" part="a_adj"/><lit-tag v="adj.sint"/></equal>
	      </and>
	    </test>
	    <let><clip pos="1" side="tl" part="lem"/> 
	      <get-case-from pos="1"><lit v="much"/></get-case-from></let>
	    <let><var n="adv_adjectiu"/>
		  <concat><clip pos="1" side="tl" part="whole"/><lit v="$ ^"/><clip pos="2" side="tl" part="lem"/><clip pos="2" side="tl" part="a_adj"/><lit-tag v="comp"/></concat></let>
	  </when>

	  <when>
	    <test>
	      <and>
			<equal caseless="yes"><clip pos="1" side="sl" part="lem"/><lit v="ben"/></equal>
			<not><equal><clip pos="2" side="sl" part="a_adj"/><lit-tag v="vblex.pp"/></equal></not>
	      </and>
	    </test>
	    <let>	      <var n="adv_adjectiu"/>
		<concat>
		  <get-case-from pos="1"><lit v="very"/></get-case-from>
		  <lit-tag v="preadv"/><lit v="$ ^"/><clip pos="2" side="tl" part="lem"/><clip pos="2" side="tl" part="a_adj"/>
		</concat>
	    </let>
	  </when>

	  <otherwise>
	    <let>
	      <var n="adv_adjectiu"/>
		<concat>
		  <clip pos="1" side="tl" part="whole"/> 
		  <lit v="$ ^"/>
		  <var n="superlatiu1"/>
		  <clip pos="2" side="tl" part="lem"/> 
		  <clip pos="2" side="tl" part="a_adj"/>
		</concat>
	    </let>
	  </otherwise>
	</choose>
</def-macro>


<!-- konvertas imperativon al infinitivo -->
<def-macro n="imp_inf" npar="1">
	<choose>
	  <when>
	    <test>
      		<equal><clip pos="1" side="tl" part="a_tns"/><lit-tag v="imp"/></equal>
	    </test>
	    <let><clip pos="1" side="tl" part="a_tns"/><lit-tag v="inf"/></let>
	  </when>
	  </choose>
</def-macro>


  </section-def-macros> 



