              </and>
            </or>
          </and>
        </test>
        <out>
          <chunk>
            <!--  <clip pos="1" part="lem"/> <lit v="^nom_conj_nom_conj_nom"/> -->
            <lit v="nom_conj_nom_conj_nom"/>
            <clip pos="1" part="tags"/>
            <lit v="{"/>
            <clip pos="1" part="content"/>
