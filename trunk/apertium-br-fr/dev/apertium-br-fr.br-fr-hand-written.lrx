<lrx>
  <rules>
    <rule>
      <match lemma="abadenn" tags="n.*"><select lemma="manifestation" tags="n.*"/></match>
      <match lemma="sevenadurel" tags="adj.*"/>
    </rule>

	
	
    <rule>
      <match lemma="den" tags="n.*"><select lemma="personne" tags="n.*"/></match>
      <match lemma="unan" tags="num.*"/>
      <match lemma="eus" tags="pr"/>
      <match lemma="an" tags="det.def.*"/>
	  </rule>

     <rule>
      <match lemma="den" tags="n.*"><select lemma="personne" tags="n.*"/></match>
      <match lemma="all" tags="adj.*"/>
	  </rule> 

	  <rule>
      <match lemma="den" tags="n.*"><select lemma="homme" tags="n.*"/></match>
      <match lemma="kentañ" tags="num.*"/>
<!-- Aqui se puede seleccionar o homme (cuando se habla de prehistoria/los primeros hombres) o personne (si se habla de las primeras personas que acaban de llegar en alguna parte) pero no se puede seleccionar gens antes de kentañ -->
	  </rule>	  
	  
   <rule>
      <match lemma="CUALQUIER NUMERO" tags="num.*"/>
      <match lemma="a" tags="pr.*"/>
      <match lemma="den" tags="n.*"><select lemma="personne" tags="n.*"/></match>
   </rule>

    <rule>
      <match lemma="dremm" tags="n.*"/>
      <match lemma="an" tags="det.*"/>
      <match lemma="den" tags="n.*"><select lemma="homme" tags="n.*"/></match>
   </rule>   
   
  <rule>
    <match lemma="poblad" tags="n.*"/>
	<match lemma="den" tags="n.*"><select lemma="homme" tags="n.*"/></match>
    </rule>

 <rule>
      <match lemma="den" tags="n.*"><select lemma="homme" tags="n.*"/></match>
    <or>
     <match lemma="difeiz" tags="adj.*"/>
     <match lemma="istorel" tags="adj.*"/>
     <match lemma="meur" tags="adj.*"/>
	 </or>
  </rule>   

 <rule>
   <or>
     <match lemma="kumuniezh" tags="n.*"/>
     <match lemma="rummad" tags="n.*"/>
   </or>
   <match lemma="den" tags="n.*"><select lemma="personne" tags="n.*"/></match>
  </rule>   

	  
	  
</lrx>

