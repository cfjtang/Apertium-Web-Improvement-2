  <pardefs>

    <!-- Punctuation -->

    <pardef n="coma">
      <e>        <p>          <l>,</l>          <r>,<s n="cm"/></r></p></e>
    </pardef>

    <pardef n="guionet">
      <e>        <p>          <l>-</l>          <r>-<s n="guio"/></r></p></e>
    </pardef>

    <pardef n="separa">
      <e>        <re>[.\?;:!]</re>        <p>          <l></l>          <r><s n="sent"/></r></p></e>
    </pardef>

    <pardef n="paresq">
      <e>        <re>[\(\[]</re>        <p>          <l/>          <r><s n="lpar"/></r></p></e>
    </pardef>

    <pardef n="pardret">
      <e>        <re>[\)\]]</re>        <p>          <l/>          <r><s n="rpar"/></r></p></e>
    </pardef>


<!-- Numerals -->

    <pardef n="numeros">
      <e>        <re>[0-9]+([., ][0-9]+)?([%°])?</re>        <p>          <l></l>          <r><s n="num"/></r></p></e>
      <e>        <re>[0-9]+([0-9]+)?</re>        <p>          <l>-ата</l>          <r><s n="num"/><s n="def"/></r></p></e>
      <e>        <re>[0-9]+([0-9]+)?</re>        <p>          <l><b/>г.</l>          <r><b/>г.<s n="num"/></r></p></e>
      <e r="LR"> <re>[0-9]+([0-9]+)?</re>        <p>          <l>г</l>          <r>г<s n="num"/></r></p></e>
      <e r="LR"> <re>[0-9]+([0-9]+)?</re>        <p>          <l>км</l>          <r>км<s n="num"/></r></p></e>
      <e r="LR"> <re>[0-9]+([0-9]+)?</re>        <p>          <l>см</l>          <r>см<s n="num"/></r></p></e>
      <e>        <re>[0-9]+([0-9]+)?</re>        <p>          <l>мм</l>          <r>мм<s n="num"/></r></p></e>
      <e r="LR"> <re>[0-9]+([0-9]+)?</re>        <p>          <l><b/>мм</l>          <r>мм<s n="num"/></r></p></e>
      <e r="LR"> <re>[0-9]+([0-9]+)?</re>        <p>          <l>гр</l>          <r>гр<s n="num"/></r></p></e>
      <e r="LR"> <re>[0-9]+([0-9]+)?</re>        <p>          <l><b/>млн</l>          <r>млн<s n="num"/></r></p></e>
      <e>        <re>[0-9]+([0-9]+)?</re>        <p>          <l>млн</l>          <r>млн<s n="num"/></r></p></e>
    </pardef>

