  <pardefs>
    <!-- Number regexes -->

    <pardef n="numeros">
      <e>
        <re>[0-9]+([.,][0-9]+)?</re>
        <p>
          <l/>
          <r><s n="num"/><s n="dig"/></r>
        </p>
      </e>
    </pardef>
    <pardef n="integer">
      <e>
        <re>[0-9]+</re>
      </e>
    </pardef>
    <pardef n="years">
      <e>
        <re>[0-9][0-9]([0-9][0-9])?</re>
        <p>
          <l/>
          <r><s n="num"/><s n="ord"/></r>
        </p>
      </e>
    </pardef>
    

