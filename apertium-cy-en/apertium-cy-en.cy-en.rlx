DELIMITERS = "<.>" "<!>" "<?>";

LIST N = n;
LIST NP = (np top) (np ant) (np cog);
LIST V = vblex vbser;
LIST Adv = adv;
LIST Num = num;
LIST CC = cnjcoo;
LIST CS = cnjsub;
LIST Interj = ij;
