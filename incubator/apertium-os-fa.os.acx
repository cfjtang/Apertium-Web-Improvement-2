<?xml version="1.0"?>
<analysis-chars>
  <!-- Make apostrophe variants equal ' -->
  <char value="'">
    <equiv-char value="’"/>
    <equiv-char value="ʼ"/>
  </char>

  <!-- Legacy values for characters with comma -->
  <char value="ӕ">
    <equiv-char value="æ"/>
  </char>

  <char value="Ӕ">
    <equiv-char value="Æ"/>
  </char>
</analysis-chars>
