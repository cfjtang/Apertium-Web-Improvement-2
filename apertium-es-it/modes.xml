      </program>
      <program name="apertium-postchunk">
        <file name="apertium-es-it.es-it.t3x"/>
        <file name="es-it.t3x.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="es-it.autogen.bin"/>
      </program>
      <program name="lt-proc -p">
        <file name="es-it.autopgen.bin"/>
      </program>
    </pipeline>
  </mode>

