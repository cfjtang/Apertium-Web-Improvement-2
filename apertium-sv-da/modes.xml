<?xml version="1.0" encoding="UTF-8"?>
<modes>
  <mode name="sv-da" install="yes">
    <pipeline>
      <program name="lt-proc">
        <file name="sv-da.automorf.bin"/>
      </program>
      <program name="apertium-tagger -g $2">
        <file name="sv-da.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="apertium-transfer">
        <file name="apertium-sv-da.sv-da.t1x"/>
        <file name="sv-da.t1x.bin"/>
        <file name="sv-da.autobil.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="sv-da.autogen.bin"/>
      </program>
      <program name="lt-proc -p">
        <file name="sv-da.autopgen.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="sv-da-tagger" install="no">
    <pipeline>
      <program name="lt-proc">
        <file name="sv-da.automorf.bin"/>
      </program>
      <program name="apertium-tagger -g $2">
        <file name="sv-da.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
    </pipeline>
  </mode>

  <mode name="sv-da-transfer" install="no">
    <pipeline>
      <program name="lt-proc">
        <file name="sv-da.automorf.bin"/>
      </program>
      <program name="apertium-tagger -g $2">
        <file name="sv-da.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="apertium-transfer">
        <file name="apertium-sv-da.sv-da.t1x"/>
        <file name="sv-da.t1x.bin"/>
        <file name="sv-da.autobil.bin"/>
      </program>
    </pipeline>
  </mode>

</modes>
