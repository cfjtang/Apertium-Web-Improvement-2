  <mode name="mk-sh_BS" install="yes">
    <pipeline>
      <program name="lt-proc">
        <file name="mk-sh.automorf.bin"/>
      </program>
      <program name="apertium-tagger -g $2">
        <file name="mk-sh.prob"/>
      </program> 
      <program name="apertium-pretransfer"/>
      <program name="lt-proc -b">
        <file name="mk-sh.autobil.bin"/>
      </program>
      <program name="apertium-transfer -b">
        <file name="apertium-sh-mk.mk-sh.t1x"/>
        <file name="mk-sh.t1x.bin"/>
      </program>
      <program name="apertium-interchunk">
        <file name="apertium-sh-mk.mk-sh.t2x"/>
        <file name="mk-sh.t2x.bin"/>
      </program>
      <program name="apertium-postchunk">
        <file name="apertium-sh-mk.mk-sh.t3x"/>
        <file name="mk-sh.t3x.bin"/>
      </program>
      <program name="lt-proc -g">
        <file name="mk-sh_BS.autogen.bin"/>
      </program>
      <program name="lt-proc -p">
        <file name="mk-sh.autopgen.bin"/>
      </program>
    </pipeline>
  </mode>

