    <pardef n="numeros">
      <e>
        <re>[0-9]+([.,][0-9]+)?</re>
        <p>
          <l/>
          <r><s n="num"/><s n="dig"/></r>
        </p>
      </e>
    </pardef>
    <pardef n="integer">
